<div class="container">
hello over here



<table class="table">
  <thead class="thead-light">
    <tr>
      <th scope="col">#ID</th>
      <th scope="col">Email</th>
      <th scope="col">Etat</th>
    </tr>
  </thead>
  <tbody>
  
  
  <c:forEach items="${userList}" var="user">
	<tr>
						<td>${user.id}</td>
						<td>${user.email}</td>
						<c:if test="${user.state == true}">
							<td>Activé</td>
						</c:if>
						<c:if test="${user.state == false}">
							<td>Desactivé</td>
						</c:if>

	</tr>
	</c:forEach>
    <tr>
      <th scope="row">1</th>
      <td>Mark</td>
      <td>Otto</td>
     
    </tr>
    <tr>
      <th scope="row">2</th>
      <td>Jacob</td>
      <td>Thornton</td>

    </tr>
    <tr>
      <th scope="row">3</th>
      <td>Larry</td>
      <td>the Bird</td>
    </tr>
  </tbody>
</table>
</div>


